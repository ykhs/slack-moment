{
  "parser": "babel-eslint",
  "extends": [
    "vue",
    "standard",
    "plugin:import/errors",
    "plugin:import/warnings"
  ],
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "plugins": [
    "vue",
    "import",
    "promise"
  ],
  "rules": {
    "prefer-const": 2,
    "no-duplicate-imports": 0,
    "promise/param-names": 0
  }
}

{
  "private": true,
  "version": "0.0.0",
  "engines": {
    "node": "6.9.4"
  },
  "scripts": {
    "lint": "eslint --ext .js,.vue src/js config app",
    "lint:fix": "eslint --fix --ext .js,.vue src/js config app",
    "start": "npm run watch",
    "clean": "rimraf public/bundle.*",
    "build:js": "webpack",
    "build:css": "postcss -c postcss.config.js",
    "build": "npm-run-all -s clean -p build:*",
    "watch:js": "webpack --watch",
    "watch:css": "chokidar --initial './src/**/*.css' -i 'public/bundle.css' -c 'npm run build:css'",
    "watch": "npm-run-all -s clean -p watch:* server:dev",
    "release:js": "cross-env NODE_ENV=production webpack",
    "release:css": "cross-env NODE_ENV=production postcss --config postcss.config.js",
    "release": "npm-run-all -s clean -p release:*",
    "server:dev": "cross-env DEBUG=slack-moment:* BABEL_ENV=server nodemon app.js --exec babel-node",
    "server:prod": "cross-env DEBUG=slack-moment:* BABEL_ENV=server NODE_ENV=production babel-node app.js",
    "heroku-postbuild": "npm run release"
  },
  "devDependencies": {
    "autoprefixer": "6.7.2",
    "babel-core": "6.22.1",
    "babel-eslint": "7.1.1",
    "babel-loader": "6.2.10",
    "babel-plugin-transform-async-to-generator": "6.22.0",
    "babel-plugin-transform-object-rest-spread": "6.23.0",
    "babel-preset-env": "1.1.8",
    "chokidar-cli": "1.2.0",
    "cross-env": "3.1.4",
    "css-loader": "0.26.2",
    "csswring": "5.1.1",
    "debug": "2.6.1",
    "dotenv": "4.0.0",
    "eslint": "3.15.0",
    "eslint-config-standard": "6.2.1",
    "eslint-config-vue": "2.0.2",
    "eslint-plugin-html": "2.0.1",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-promise": "3.4.2",
    "eslint-plugin-standard": "2.0.1",
    "eslint-plugin-vue": "2.0.1",
    "morgan": "1.8.1",
    "nodemon": "1.11.0",
    "npm-run-all": "4.0.1",
    "postcss-cli": "2.6.0",
    "postcss-easy-import": "2.0.0",
    "postcss-reporter": "3.0.0",
    "rimraf": "2.5.4",
    "uglify-js": "git://github.com/mishoo/UglifyJS2#harmony",
    "uglifyjs-webpack-plugin": "0.1.3",
    "vue-loader": "latest",
    "vue-template-compiler": "latest",
    "webpack": "2.2.1",
    "webpack-dev-server": "2.3.0"
  },
  "dependencies": {
    "@slack/client": "3.8.1",
    "axios": "0.15.3",
    "babel-cli": "6.23.0",
    "babel-polyfill": "6.23.0",
    "basic-auth-connect": "1.0.0",
    "body-parser": "1.16.1",
    "bootstrap": "4.0.0-alpha.6",
    "cookie-parser": "1.4.3",
    "express": "4.14.1",
    "fecha": "2.3.0",
    "firebase": "3.6.10",
    "firebase-admin": "4.1.1",
    "hover.css": "2.0.2",
    "js-cookie": "2.1.3",
    "nprogress": "0.2.0",
    "pify": "2.3.0",
    "pug": "2.0.0-beta11",
    "simple-oauth2": "1.0.3",
    "vue": "2.2.0",
    "vue-infinite-loading": "2.0.2",
    "vue-router": "2.2.1",
    "vuex": "2.1.3",
    "vuex-router-sync": "4.1.2"
  }
}

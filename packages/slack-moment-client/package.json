{
  "name": "slack-moment-client",
  "license": "MIT",
  "version": "0.1.0",
  "scripts": {
    "clean": "rimraf public/**/bundle.*",
    "build:js": "webpack --config webpack/webpack.dev.config.js",
    "build:css": "postcss -c postcss.config.js",
    "build": "npm-run-all -s clean -p build:*",
    "watch:js": "webpack --watch --config webpack/webpack.dev.config.js",
    "watch:css": "postcss -c postcss.config.js -w",
    "watch": "npm-run-all -s clean -p watch:*",
    "release:js":
      "cross-env NODE_ENV=production webpack --config webpack/webpack.prod.config.js",
    "release:css":
      "cross-env NODE_ENV=production postcss --config postcss.config.js",
    "release": "npm-run-all -s clean -p release:*"
  },
  "dependencies": {
    "axios": "0.16.2",
    "babel-polyfill": "6.26.0",
    "bootstrap": "4.0.0-alpha.6",
    "fecha": "2.3.1",
    "firebase": "4.5.1",
    "hover.css": "2.2.1",
    "js-cookie": "2.1.4",
    "nprogress": "0.2.0",
    "vue": "2.5.2",
    "vue-infinite-loading": "2.2.1",
    "vue-router": "3.0.1",
    "vuex": "3.0.0",
    "vuex-router-sync": "5.0.0"
  },
  "devDependencies": {
    "autoprefixer": "7.1.5",
    "css-loader": "0.28.7",
    "csswring": "6.0.1",
    "postcss-cli": "4.1.1",
    "postcss-easy-import": "3.0.0",
    "postcss-reporter": "5.0.0",
    "vue-loader": "13.3.0",
    "vue-template-compiler": "2.5.2"
  }
}
